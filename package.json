{
  "name": "react-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.93.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "set-dev-env": "env-cmd -f ./env/.env.dev",
    "set-staging-env": "env-cmd -f ./env/.env.staging",
    "set-prod-env": "env-cmd -f ./env/.env.prod",
    "dev": "yarn set-dev-env react-app-rewired start",
    "staging": "yarn set-staging-env react-app-rewired start",
    "prod": "yarn set-prod-env react-app-rewired start",
    "build:dev": "yarn set-dev-env react-app-rewired build",
    "build:staging": "yarn set-staging-env react-app-rewired build",
    "build:prod": "yarn set-prod-env react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "prettier:check": "npx prettier --check ./",
    "prettier:fix": "npx prettier --write ./",
    "eslint:check": "npx eslint .",
    "eslint:fix": "npx eslint --fix .",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-onboarding": "^9.1.8",
    "@storybook/preset-create-react-app": "^9.1.8",
    "@storybook/react-webpack5": "^9.1.8",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "env-cmd": "^11.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^9.1.8",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react-app-rewire-alias": "^1.1.7",
    "react-app-rewired": "^2.2.1",
    "storybook": "^9.1.8",
    "webpack": "^5.101.3"
  }
}